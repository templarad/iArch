interface component Subject {
	void addObserver(Observer observer);
	void removeObserver(Observer observer);
	void _notify();
	String getState();
	void setState(String state);
}

interface component Observer {
	void update();
}

Subject = (Subject.setState->Subject._notify->
	Observer.update->Subject.getState->Subject
);
	
Observer = (Observer.update->Subject.getState
	->Observer);